import java.util.Scanner;

public class Main {
	static float raio = 3.14159f;
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
			
		int A, B, C, D, diferenca;
		
		A = scanner.nextInt();
		B = scanner.nextInt();
		C = scanner.nextInt();
		D = scanner.nextInt();
		
		diferenca = A * B - C * D;
		
		System.out.println("Diferença é: " + diferenca);
		scanner.close();
	}
}