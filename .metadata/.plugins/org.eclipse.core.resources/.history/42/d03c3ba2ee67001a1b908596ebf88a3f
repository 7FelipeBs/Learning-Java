import java.util.Scanner;

public class Main {
	static float raio = 3.14159f;
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
			
			double n1, result;
			
			n1 = scanner.nextDouble();
			
			result = raio * n1 * n1;
			
			System.out.printf("Result: %.4f%n", result);
		scanner.close();
	}
}