package Entities;

public abstract class Account {

	public String nameHolder;
	protected int numberCard;
	protected int keyValidation;
	protected double balanceAccount;
	
	public String getNameHolder() {
		return nameHolder;
	}
	public void setNameHolder(String nameHolder) {
		this.nameHolder = nameHolder;
	}
	public int getNumberCard() {
		return numberCard;
	}
	public void setNumberCard(int numberCard) {
		this.numberCard = numberCard;
	}
	public int getKeyValidation() {
		return keyValidation;
	}
	public void setKeyValidation(int keyValidation) {
		this.keyValidation = keyValidation;
	}
	public double getBalanceAccount() {
		return balanceAccount;
	}
	public void setBalanceAccount(double balanceAccount) {
		this.balanceAccount = balanceAccount;
	}

	protected Account(String nameHolder, int numberCard, int keyValidation, double balanceAccount) {
		super();
		this.nameHolder = nameHolder;
		this.numberCard = numberCard;
		this.keyValidation = keyValidation;
		this.balanceAccount = balanceAccount;
	}
	
	 public void withdraw(double amount) {
	 };
	  
	 public void deposit(double amount) {
	 };
	  
}
