package Entities;

public class AccountBusiness extends Account {

	protected AccountBusiness(String nameHolder, int numberCard, int keyValidation, double balanceAccount) {
		super(nameHolder, numberCard, keyValidation, balanceAccount);
		
	}

	@Override
	public void withdraw(double amount) {
		super.withdraw(amount);
		balance -= 50 + amount;
	}

	@Override
	public void deposit(double amount) {
		super.deposit(amount);
	}

}
